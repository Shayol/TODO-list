      <div class="row equal" id="project_row_<%= project.id %>">
        <div class="col-md-1 col-md-offset-2">
          Calendar
        </div>
        <div class="col-md-5 project-title"><%= project.title %></div>
        <div class="col-md-1">
          <%= link_to edit_project_path(project), remote: true do %>
            <i class="fa fa-pencil"></i>
          <% end %>
          <%= link_to project, method: :delete, data: { confirm: 'Are you sure?' }  do%>
            <i class="fa fa-trash"></i>
          <% end %>
        </div>
      </div>