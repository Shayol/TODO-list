      <div class="row" id="project_row_<%= project.id %>">
        <div class="col-md-1 col-md-offset-2">
          Calendar
        </div>
        <div class="col-md-4"><h3><%= project.title %></h3></div>
        <div class="col-md-2">
          <%= link_to edit_project_path(project), remote: true do %>
            <i class="fa fa-pencil"></i>
          <% end %>|
          <%= link_to project, method: :delete, data: { confirm: 'Are you sure?' }  do%>
            <i class="fa fa-trash"></i>
          <% end %>
        </div>
      </div>